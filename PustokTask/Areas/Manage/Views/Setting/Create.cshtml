@model Setting

<h2>Create Setting</h2>
<form method="post">
    <div class="form-group">
        <label>Key</label>
        <input asp-for="Key" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Value</label>
        <input asp-for="Value" class="form-control" required />
    </div>
    <button type="submit" class="btn btn-success">Save</button>
</form>
