@model List<Setting>

<h2>Settings</h2>
<a asp-action="Create" class="btn btn-primary">New Setting</a>

<table class="table">
    <tr>
        <th>Key</th>
        <th>Value</th>
        <th>Setting</th>
    </tr>
    @foreach (var setting in Model)
    {
        <tr>
            <td>@setting.Key</td>
            <td>@setting.Value</td>
            <td>
                <a class="btn btn-primary" asp-action="edit" asp-controller="Setting" asp-route-id="@setting.Id">
                    Edit
                </a>

                <a asp-area="Manage" asp-action="Delete" asp-controller="Setting" asp-route-id="@setting.Id" class="btn btn-danger deletSetting">
                    Delete
                </a>
            </td>
        
        </tr>
    }
</table>
